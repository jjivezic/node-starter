{
  "name": "hyper-server",
  "version": "1.0.0",
  "description": "Node.js server with JWT authentication and MySQL",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "chroma": "export PATH=\"$HOME/Library/Python/3.9/bin:$PATH\" && chroma run --path ./chroma_data --host localhost --port 8000",
    "chroma:stop": "pkill -f chroma",
    "db:migrate": "npx sequelize-cli db:migrate --config database/config.cjs",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo --config database/config.cjs",
    "db:seed": "npx sequelize-cli db:seed:all --config database/config.cjs",
    "db:seed:undo": "npx sequelize-cli db:seed:undo:all --config database/config.cjs",
    "db:reset": "npm run db:migrate:undo && npm run db:migrate && npm run db:seed",
    "generate:model": "node database/schema-generator.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.js\" \"database/**/*.js\"",
    "ingest:drive": "node src/services/driveIngestionService.js"
  },
  "keywords": [
    "express",
    "mysql",
    "jwt",
    "authentication"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.958.0",
    "@aws-sdk/s3-request-presigner": "^3.958.0",
    "@google/generative-ai": "^0.24.1",
    "bcrypt": "^5.1.1",
    "chromadb": "^3.2.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "docx4js": "^3.3.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.5.2",
    "express-rate-limit": "^8.2.1",
    "googleapis": "^169.0.0",
    "helmet": "^8.1.0",
    "joi": "^18.0.2",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.11.0",
    "morgan": "^1.10.1",
    "mysql2": "^3.6.5",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.12",
    "openai": "^6.15.0",
    "pdf-parse": "^2.4.5",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.7.4"
  }
}
